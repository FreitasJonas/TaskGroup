@using TaskGroupWeb.Helpers

@if (TempData[OperationResult.Error.ToString()] != null && !string.IsNullOrEmpty(TempData[OperationResult.Error.ToString()].ToString()))
{
    <p class="alert alert-danger">@TempData[OperationResult.Error.ToString()]</p>
}
else if (TempData[OperationResult.Success.ToString()] != null && !string.IsNullOrEmpty(TempData[OperationResult.Success.ToString()].ToString()))
{
    <p class="alert alert-success">@TempData[OperationResult.Success.ToString()]</p>
}